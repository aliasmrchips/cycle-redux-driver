{
  "name": "@sunny-g/cycle-redux-driver",
  "version": "0.1.1",
  "description": "(u)nofficial Redux Action and State driver for Cycle.js",
  "author": "Sunny G <sunny.gonna@gmail.com> (http://blog.sunnyg.io)",
  "homepage": "https://github.com/sunny-g/cycle-redux-driver",
  "repository": "https://github.com/sunny-g/cycle-redux-driver",
  "bugs": "https://github.com/sunny-g/cycle-redux-driver/issues",
  "license": "ISC",
  "main": "index.js",
  "types": "dist/src/index.d.ts",
  "keywords": [
    "cycle",
    "redux",
    "rxjs",
    "mostjs",
    "xstream"
  ],
  "scripts": {
    "build": "webpack",
    "build:watch": "npm run build -- --watch",
    "prepublish": "npm run build -- -p",
    "test": "jest --no-cache",
    "test:watch": "npm run test -- --watch"
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      }
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/test/*.test.(ts|js)"
    ],
    "testResultsProcessor": "<rootDir>/node_modules/ts-jest/coverageprocessor.js",
    "transform": {
      "\\.(ts|js)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  },
  "dependencies": {
    "@cycle/run": "*",
    "flux-standard-action": "^1.2.0",
    "redux": "^3.6.0",
    "xstream": "^10.3.0"
  },
  "devDependencies": {
    "@types/jest": "^19.2.2",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.4.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "jest": "^19.0.2",
    "jest-cli": "^19.0.2",
    "ts-jest": "^19.0.1",
    "ts-loader": "^2.0.1",
    "typescript": "^2.2.1",
    "webpack": "^2.3.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
